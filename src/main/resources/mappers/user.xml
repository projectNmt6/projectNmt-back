<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.projectnmt.projectnmt.repository.UserMapper">

    <select id="findUserTest" resultType="com.projectnmt.projectnmt.entity.User">
        select
            user_id,
            username,
            password,
            name,
            phone_number,
            email,
            gender,
            age,
            profile_img
        from
            user_tb
        where
            user_id = 1;
    </select>


</mapper>